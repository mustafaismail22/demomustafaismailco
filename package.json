    "classnames": "^2.2.5",
    "history": "^4.6.1",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.5.4",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.2",
    "react-slick": "^0.14.7",
    "slick-carousel": "^1.6.0"
