{
  "name": "mustafaismailco",
  "description": "Source code for my personal site [demo.mustafaismail.co](http://demo.mustafaismail.co/)",
  "version": "1.0.0",
  "author": "Mustafa ismail",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && npm run watch",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --colors --display-error-details --display-reasons --config ./config/webpack.config.js",
    "build": "npm run clean && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors --display-error-details --display-reasons --config ./config/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack -p --progress --colors --display-error-details --display-reasons --config ./config/webpack.config.js",
    "clean": "rimraf ./build",
    "render": "cross-env NODE_ENV=production babel-node ./scripts/renderToHtml.js",
    "lint": "eslint ./src ./scripts ./config; exit 0",
    "deploy": "npm run render && gh-pages -d ./build"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-react-app": "^2.2.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-config-standard": "^10.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^0.12.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-manifest-plugin": "^1.1.0"
  },
